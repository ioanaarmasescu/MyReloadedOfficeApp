@model IEnumerable<MyReloadedOfficeApp.Models.UsersClustersModel>

@{
    ViewBag.Title = "IndexError";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>IndexError</h2>

@if (ViewBag.Message != null)
{
    <script type="text/javascript">

            window.onload = function () {
                alert("@ViewBag.Message");
            };
    </script>

}
<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.IdDepartment)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.IdUserType)
        </th>
        <th>
            @*Html.DisplayNameFor(model => model.Discriminator)*@
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdDepartment)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdUserType)
            </td>
            <td>
                @*Html.DisplayFor(modelItem => item.Discriminator)*@
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.Id }) |
                @Html.ActionLink("Details", "Details", new { id = item.Id }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.Id })
            </td>
        </tr>
    }

</table>
